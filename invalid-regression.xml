<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="GitHub">
    <test name="InvalidGitHubSignIn">
        <parameter name="browserName" value="${browserName}"></parameter>
        <groups>
            <run>
                <include name="invalid-regression"/>
            </run>
        </groups>
        <packages>
            <package name="GitHubTests"></package>
        </packages>
    </test>
</suite>